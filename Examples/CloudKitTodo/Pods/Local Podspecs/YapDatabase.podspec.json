{
  "name": "YapDatabase",
  "version": "3.0.1",
  "summary": "A key/value store built atop sqlite for iOS & Mac.",
  "homepage": "https://github.com/yapstudios/YapDatabase",
  "license": "MIT",
  "authors": {
    "Robbie Hanson": "robbiehanson@deusty.com"
